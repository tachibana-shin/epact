{
  "name": "epact",
  "description": "EPACT (Express Package Advanced Typescript)",
  "author": "Tachibana Shin",
  "version": "0.0.9-beta.20",
  "main": "./dist/index.js",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tachibana-shin/epact.git"
  },
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "tsup-node",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.18.1",
    "tsup": "^6.0.1",
    "vitest": "^0.12.9"
  },
  "bugs": {
    "url": "https://github.com/tachibana-shin/epact/issues"
  },
  "homepage": "https://github.com/tachibana-shin/epact#readme"
}
